<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!-- specify the index of individual SISO controller, controllers with excessive indices will be ignored -->
<!-- filter: 1 = Not used, 2 = Low pass, 3 = High pass, 4 = (Skew) Notch -->

<!-- Note: AxisTrans uses a SISO controller -->
<Parameters>
    <FeedBackController>
        <PID1>
            <Kp Value="45918"/>
            <FrqI Value="25"/>
            <FrqD Value="15"/>
            <BetaD Value="0.1"/>
        </PID1>
        <PID2>
            <Kp Value="1"/>
            <FrqI Value="25"/>
            <FrqD Value="15"/>
            <BetaD Value="0.1"/>
        </PID2>
        <Filter1>
            <Type Value="4"/>
            <Frq0 Value="1035"/>
            <Mag0 Value="20"/>
            <Frq1 Value="1035"/>
            <Mag1 Value="15"/>
        </Filter1>
        <Filter2>
            <Type Value="4"/>
            <Frq0 Value="1500"/>
            <Mag0 Value="6"/>
            <Frq1 Value="1500"/>
            <Mag1 Value="1"/>
        </Filter2>  
    </FeedBackController>
</Parameters>